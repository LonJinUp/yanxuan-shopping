/* pages/MyProfessor/MyProfessor.wxss */
/* remind */
page{
  width: 100%;
  height: 100%;
  background: #FFFFFF;
}
.remind{
  width:100%;
  height:68rpx;
  background:rgba(217,189,140,1);
  box-sizing: border-box;
  padding: 0 30rpx;
  display: flex;
  align-items: center;
}
.remind image{
  width:32rpx;
  height:32rpx;
}
.remind text{
  font-size:22rpx;
  font-family:PingFangSC-Light;
  font-weight:300;
  color:rgba(255,255,255,1);
  margin-left:20rpx;
}
/* remind end */
/* Professor-title */
.Professor-title{
  width: 100%;
  height: 134rpx;
  font-size:26rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(0,0,0,1);
  line-height:40rpx;
  box-sizing: border-box;
  padding: 20rpx 30rpx;
  border-bottom: 1rpx solid #eeeeee;
}
/* Professor-title end */

/* no-professor */
.no-professor{
  width:442rpx;
  height:344rpx;
  margin: 0 auto;
  margin-top:182rpx;
  background: url('data:image/png;base64,')no-repeat center center /100% 100%;
}
.no-professor-title{
  width:448rpx;
  height:71rpx;
  font-size:30rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(187,187,187,1);
  line-height:42rpx;
  margin: 0 auto;
  margin-top: 40rpx;
  text-align: center;
}
/* no-professor end */

/* weChat */
.weChat{
  width:100%;
  height:100rpx;
  background:rgba(255,255,255,1);
  border-bottom: 1rpx solid #eeeeee;
  display: flex;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  padding: 0 28rpx;
}
.weChat .left{
  width: 80%;
  display: flex;
  align-items: center;
}
.weChat .left text:first-child{
  font-size:30rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(0,0,0,1);
}
.weChat .left input{
  font-size:30rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(110,110,110,1);
  margin-left: 70rpx;
}
.weChat .right{
  width: 26rpx;
  height: 100%;
  font-size:26rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(255,16,16,1);
}
/* weChat end */

/* my-professor */
.my-professor{
  width:100%;
  height:100rpx;
  background:rgba(255,255,255,1);
  box-sizing: border-box;
  padding: 0 28rpx;
}
.my-professor .title{
  width:267rpx;
  height:30rpx;
  font-size:30rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(0,0,0,1);
  margin-top: 35rpx;
}
.my-professor .content{
  width:379rpx;
  height:20rpx;
  font-size:20rpx;
  font-family:PingFangSC-Light;
  font-weight:300;
  color:rgba(187,187,187,1);
  margin-top: 16rpx;
}
.weChat-img{
  width: 400rpx;
  height: 400rpx;
  margin: 0 auto;
  margin-top: 50rpx;
  background:rgba(250,250,250,1);
  border:1rpx solid rgba(187, 187, 187, 1);
  border-radius:10rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.weChat-img image{
  width:56rpx;
  height:56rpx;
}
.weChat-img2{
  width: 400rpx;
  height: 400rpx;
  margin: 0 auto;
  margin-top: 50rpx;
  background:rgba(250,250,250,1);
  border:1rpx solid rgba(187, 187, 187, 1);
  border-radius:10rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.weChat-img2 image{
 width: 100%;
 height: 100%;
}
