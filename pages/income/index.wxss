/* pages/SecondaryPage/MyIncome/index.wxss */
/* income */
page{
  width: 100%;
  height: 100%;
}
.income{
  width:690rpx;
  height:300rpx;
  background:linear-gradient(90deg,rgba(247,63,80,1),rgba(248,82,129,1));
  border-radius:10rpx;
  margin:0 auto;
  overflow: hidden;
  margin-top: 30rpx;
}
.income .income-title{
  width: 100%;
  display: flex;
  align-items: center;
  margin-top: 56rpx;
  box-sizing: border-box;
  padding: 0 50rpx;

}
.income .income-title .left{
  width:31rpx;
  height:35rpx;
  float: left;
}
.income .income-title .left image{
  width: 100%;
  height: 100%;
}
.income .income-title .right{
  box-sizing: border-box;
  padding-top: 20rpx;
  font-size:28rpx;
  font-family:PingFangSC-Light;
  font-weight:300;
  color:rgba(255,255,255,1);
  margin-left: 12rpx;
}
.income .money{
  font-size:60rpx;
  font-family:PingFangSC-Medium;
  font-weight:500;
  color:rgba(255,255,255,1);
  box-sizing: border-box;
  padding: 0 50rpx;
}
.income .money-day{
  width: 100%;
  height: 30rpx;
  margin-top: 44rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  
}
.income .money-day .items{

  width: 33.3%;
  height: 100%;
  font-size:22rpx;
  font-family:PingFangSC-Light;
  font-weight:300;
  color:rgba(255,255,255,1);
  text-align: center;
  line-height: 30rpx;
  border-right:1rpx solid rgba(255,255,255,1);
}
.income .money-day .items:last-child{
  border-right:none;
}

/* income end */

/* income-items */

.income-items{
  width:100%;
  height:140rpx;
  background:rgba(255,255,255,1);
  border-bottom: 1rpx solid #eeeeee;
  box-sizing: border-box;
  padding: 20rpx 30rpx;
}
.income-items .left{
  width:100rpx;
  height:100rpx;
  border-radius:50%;
  margin-right: 20rpx;
  float: left;
}
.income-items .left image{
  width:100%;
  height:100%;
  border-radius:50%;
}
.income-items .center{
  width: 300rpx;
  height: 100%;
  float: left;
}
.income-items .center view:first-child{
  font-size:22rpx;
  font-family:PingFangSC-Light;
  font-weight:300;
  color:rgba(110,110,110,1);
  margin-top: 16rpx;
}
.income-items .center view:last-child{
  font-size:22rpx;
  font-family:PingFangSC-Light;
  font-weight:300;
  color:rgba(110,110,110,1);
  margin-top: 30rpx;
}
.income-items .right{
  min-width:93rpx;
  height:100%;
  float: right;
  font-size:40rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(0,0,0,1);
  line-height: 110rpx;
}
/* income-items end */

/* withdraw */
.withdraw{
  width: 100%;
  height: 150rpx;
  box-sizing: border-box;
  padding: 0 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-top: 15rpx;
}

.withdraw .left{
  width: 50%;
  height: 100%;
  box-sizing: border-box;
  padding-top: 14rpx;
}
.withdraw .left text:first-child{
  font-size:28rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(0,0,0,1);
  
}
.withdraw .left text:nth-child(2){
  font-size:20rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(110,110,110,1);
}
.withdraw .left view{
  font-size:48rpx;
  font-family:PingFangSC-Medium;
  font-weight:500;
  color:rgba(255,16,16,1);
  
}
.withdraw .right{
  width:180rpx;
  height:72rpx;
  background:rgba(255,255,255,0);
  border:2rpx solid rgba(255, 16, 16, 1);
  border-radius:36rpx;
  text-align: center;
  line-height: 72rpx;
  font-size:26rpx;
  font-family:PingFangSC-Medium;
  font-weight:500;
  color:rgba(255,16,16,1);
}
/* withdraw end */
.border{
  width:100%;
height:16rpx;
background:rgba(246,246,246,1);
}
/* Certification */
.Certification{
  width: 100%;
  height:66rpx;
  background:rgba(255,255,255,1);
  border-bottom: 1rpx solid #eeeeee;
  box-sizing: border-box;
  padding: 0 30rpx;
  display: flex;
  align-items: center;
}
.Certification .icon{
  float: left;
  width:32rpx;
  height:32rpx;
  background: url('data:image/png;base64,')no-repeat center center /100% 100%;
}
.Certification .title{
  width: 80%;
  height: 100%;
  font-size:22rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(0,0,0,1);
  line-height: 66rpx;
  margin-left: 20rpx;
}

/* Certification end */

movable-area {
  height:100%;
  width:100%;
}
.choose{
  width:88rpx;
  height:88rpx;
  background:rgba(255,255,255,1);
  box-shadow:0rpx 0rpx 27rpx 0rpx rgba(0, 0, 0, 0.08);
  border-radius:50%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.choose image{
  width:30rpx;
  height:34rpx;
}


/* showModal */
.showModal-box{
  position:absolute;
  top: 0;
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.5;
  transition: all .6s;
  z-index: 1;
}
.showModal{
  position:fixed;
  bottom: 0;
  right: 0;
 width:100%;
  height:400rpx;
  background:rgba(255,255,255,1);
  border-radius:30rpx 30rpx 0rpx 0rpx;
  z-index:2;
}
.showModal .clone{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  bottom:551rpx;
  right: 31rpx;
}
.showModal .clone image{
  width: 30rpx;
  height: 30rpx;
}
.showModal .title{
  width:110rpx;
  height:27rpx;
  font-size:28rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(0,0,0,1);
  line-height:34rpx;
  margin:0 auto;
  margin-top: 50rpx;
  text-align: center;
}
.showModal .choose-content{
  width: 100%;
  overflow: hidden;
  box-sizing: border-box;
  padding: 0 30rpx;
  margin-top: 40rpx;
}
.showModal .choose-content .items{
  width:180rpx;
  height:60rpx;
  background:rgba(255,255,255,0);
  border:1rpx solid rgba(187, 187, 187, 1);
  border-radius:6rpx;
  text-align: center;
  font-size:20rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(0,0,0,1);
  line-height:60rpx;
  margin-right: 30rpx;
  margin-bottom: 30rpx;
  float: left;
}
.showModal .choose-content .items.active{
  color: #FF3232;
  border: 1rpx solid red;
}
.back-btn{
  width:100%;
  height:100rpx;
  background:rgba(255,255,255,1);
  text-align: center;
  line-height: 100rpx;
  font-size:28rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(0,0,0,1);
  border-top: 1rpx solid rgba(238,238,238,1);
   
}

  
/* showModal end */
