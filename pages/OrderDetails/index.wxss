/* pages/SecondaryPage/MyIncome/index.wxss */
/* income */
page{
  width: 100%;
  height: 100%;
}
.border{
  width: 100%;
  height:16rpx;
 background:rgba(246,246,246,1);
}
.income{
  width:690rpx;
  height:300rpx;
  background:linear-gradient(90deg,rgba(247,63,80,1),rgba(248,82,129,1));
  border-radius:10rpx;
  margin:0 auto;
  overflow: hidden;
  margin-top: 30rpx;
}
.income .income-title{
  width: 100%;
  display: flex;
  align-items: center;
  margin-top: 56rpx;
  box-sizing: border-box;
  padding: 0 50rpx;

}
.income .income-title .left{
  width:29rpx;
  height:35rpx;
  float: left;
}
.income .income-title .left image{
  width: 100%;
  height: 100%;
}
.income .income-title .right{
  box-sizing: border-box;
  padding-top: 20rpx;
  font-size:28rpx;
  font-family:PingFangSC-Light;
  font-weight:300;
  color:rgba(255,255,255,1);
  margin-left: 12rpx;
}
.income .money{
  font-size:60rpx;
  font-family:PingFangSC-Medium;
  font-weight:500;
  color:rgba(255,255,255,1);
  box-sizing: border-box;
  padding: 0 50rpx;
}
.income .money-day{
  width: 100%;
  height: 30rpx;
  margin-top: 44rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  
}
.income .money-day .items{
  width: 33.3%;
  height: 100%;
  font-size:22rpx;
  font-family:PingFangSC-Light;
  font-weight:300;
  color:rgba(255,255,255,1);
  text-align: center;
  line-height: 30rpx;
  border-right:1rpx solid rgba(255,255,255,1);
}
.income .money-day .items:last-child{
  border-right:none;
}
/* income end */

/* swiper-tab */
.swiper-tab{
  width:100%;
  height:100rpx;
  background:rgba(255,255,255,1);
  text-align: center;
  line-height: 100rpx;
  display: flex;
  flex-flow: row;
  justify-content: space-between;
  margin-top: 10rpx;
}
.swiper-tab-item{
  width: 30%; 
  font-size:26rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:#6E6E6E;
  position: relative;
}
.swiper-tab-item .bottom-border{
  position: absolute;
  bottom: 0;
  left: 0 ;
  right: 0;
  margin: 0 auto;
  width:44rpx;
  height:6rpx;
  background:rgba(255,16,16,1);
  border-radius:3rpx;
  display: none;
}
.swiper-tab-item .bottom-border.active{
  display: block;
}
.active{
  color:#000000;
  /* border-bottom: 4rpx solid #F65959; */
}
swiper{
  text-align: center;
  height: 100%;
}
/* swiper-tab end */

/* hide money */
.hide-box{
  width:100%;
  height:68rpx;
  background:#ffd1d2;
  box-sizing: border-box;
  padding: 0 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.hide-box text{
  font-size:22rpx;
  font-family:PingFangSC-Light;
  font-weight:300;
  color:#000000;
}
.hide-box .hide{
  width:150rpx;
  height:46rpx;
  background:rgba(255,27,28,0);
  border:1rpx solid rgba(0, 0, 0, 1);
  border-radius:23rpx;
  text-align: center;
  line-height: 46rpx;
  font-size:22rpx;
  font-family:PingFangSC-Light;
  font-weight:300;
  color:rgba(0,0,0,1);
}
/* hide money end */

/* Product list */
.Product-list{
  width: 100%;
  height: auto;
  text-align: left;
} 
.product{
  width: 100%;
  height: auto;
  box-sizing: border-box;
  padding: 0 30rpx;
}
.product .title{
  width:100%;
  height: 52rpx;
  margin-top: 40rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;

}
.product .title .left{
  display: flex;
  align-items:center;
}
.product .title .left image{
  width:52rpx;
  height:52rpx;
  border-radius:50%;
  margin-right: 20rpx;
}
.product .title .left text{
  font-size:26rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(0,0,0,1);
  line-height:52rpx;
}
.product .title .right{
  font-size:26rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(187,187,187,1);
  line-height:52rpx;
}
.product .content{
  width:100%;
  height:150rpx;
  margin-top: 30rpx;
  overflow: hidden;
  display: flex;
  justify-content: space-between;
  margin-bottom: 20rpx;
}
.product .content .left{
  width:140rpx;
  height:140rpx;
  border:1rpx solid rgba(238, 238, 238, 1);
  border-radius:6rpx;
  box-sizing: border-box;
  padding:1rpx;
}
.product .content .left image{
  width: 100%;
  height: 100%;
  border-radius:6rpx;
}
.product .content .right{
  width: 516rpx;
  height: 100%;
}
.product .content .right .name{
  width:517rpx;
  height:64rpx;
  font-size:26rpx;
  font-family:PingFangSC-Light;
  font-weight:300;
  color:rgba(102,102,102,1);
  line-height:34rpx;
  overflow:hidden; 
  text-overflow:ellipsis;
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2; 
}
.product .content .right .size text:first-child{
  width:134rpx;
  height:20rpx;
  font-size:20rpx;
  font-family:PingFangSC-Light;
  font-weight:300;
  color:rgba(187,187,187,1);
  line-height:34rpx;
}
.product .content .right .price {
  width:100%;
  height:19rpx;
  display: flex;
  justify-content: space-between;
}
.product .content .right .price text{
  font-size:26rpx;
  font-family:DINCond-Black;
  font-weight:400;
  color:rgba(0,0,0,1);
  line-height:34rpx;

}
.content-text{
  width:100%;
  font-size:26rpx;
  font-family:PingFangSC-Light;
  font-weight:300;
  color:rgba(1,1,1,1);
  text-align: right;
  float: right;
  padding-bottom: 30rpx;
  border-bottom: 1rpx solid rgba(246,246,246,1);
  
}
.content-text text:first-child{
  text-align: right;
}
.content-text text:last-child{
  text-align: right;
}
.commission{
  width: 100%;
  height: 106rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.commission .left{
  width: 50%;
  font-size:22rpx;
  font-family:PingFangSC-Light;
  font-weight:300;
  color:rgba(110,110,110,1);
  line-height: 106rpx;
}
.commission .right{
  width: 50%;
  
}
.commission .right text:last-child{
  display: block;
  font-size:26rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(1,1,1,1);
  text-align: right;
  float: right;
  line-height: 106rpx;
}
.commission .right text:first-child{
  display: block;
  font-size:26rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:#FF1010;
  text-align: right;
  float: right;
  line-height: 106rpx;
}
/* Product list end */
