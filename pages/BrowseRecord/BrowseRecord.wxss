/* pages/BrowseRecord/BrowseRecord.wxss */
/* swiper-tab */
page{
  width: 100%;
  height: 100%;
}
.swiper-tab{
  width:100%;
  height:100rpx;
  background:rgba(255,255,255,1);
  text-align: center;
  line-height: 100rpx;
  display: flex;
  flex-flow: row;
  justify-content: space-between;
  margin-top: 10rpx;
}
.swiper-tab-item{
  width: 50%; 
  font-size:26rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:#6E6E6E;
  position: relative;
}
.swiper-tab-item .bottom-border{
  position: absolute;
  bottom: 0;
  left: 0 ;
  right: 0;
  margin: 0 auto;
  width:44rpx;
  height:6rpx;
  background:rgba(255,16,16,1);
  border-radius:3rpx;
  display: none;
}
.swiper-tab-item .bottom-border.active{
  display: block;
}
.active{
  color:#000000;
  /* border-bottom: 4rpx solid #F65959; */
}
swiper{
  text-align: center;
  height: 100%;
}
/* swiper-tab end */

/* hide money */
.hide-box{
  width:100%;
  height:68rpx;
  background:#ffd1d2;
  box-sizing: border-box;
  padding: 0 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.hide-box text{
  font-size:22rpx;
  font-family:PingFangSC-Light;
  font-weight:300;
  color:#000000;
}
.hide-box .hide{
  width:150rpx;
  height:46rpx;
  background:rgba(255,27,28,0);
  border:1rpx solid rgba(0, 0, 0, 1);
  border-radius:23rpx;
  text-align: center;
  line-height: 46rpx;
  font-size:22rpx;
  font-family:PingFangSC-Light;
  font-weight:300;
  color:rgba(0,0,0,1);
}
/* hide money end */

/* no-pic */
.no-pic{
  width: 303rpx;
  height: 301rpx;
  background: url('data:image/png;base64,')no-repeat center center /100% 100%;
  margin:0 auto;
  margin-top: 160rpx;
}
.no-title{
  width:100%;
  height:30rpx;
  margin-top: 45rpx;
  text-align: center;
  font-size:30rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(0,0,0,1);
}
.no-title2{
  width:100%;
  height:26rpx;
  font-size:26rpx;
  text-align: center;
  margin-top: 20rpx;
  font-family:PingFangSC-Light;
  font-weight:300;
  color:rgba(110,110,110,1);
}
.back-index{
  width:400rpx;
  margin: 0 auto;
  margin-top: 50rpx;
  height:72rpx;
  background:rgba(255,255,255,0);
  border:1rpx solid rgba(255, 16, 16, 1);
  border-radius:36rpx;
  text-align: center;
  line-height: 72rpx;
  font-size:26rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(255,16,16,1);
}
/* no-pic end */

/* list-box */
.list-box{
  width:100%;
  height:140rpx;
  background:rgba(255,255,255,1);
  box-sizing: border-box;
  padding: 0 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1rpx solid #EEEEEE;
}
.list-box .left{
  width: 50%;
  height: 100rpx;
}
.list-box .left image{
  width:100rpx;
  height:100rpx;
  border-radius:50%;
  float: left;
  margin-right:20rpx;
}
.list-box .left text{
  line-height: 100rpx;
  font-size:30rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(0,0,0,1);
  float: left;
}
.list-box .right{
  font-size:24rpx;
  font-family:PingFangSC-Light;
  font-weight:300;
  color:rgba(0,0,0,1);
}

/* list-box end */

