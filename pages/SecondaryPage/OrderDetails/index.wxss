/* order status */
.order-status{
  width: 100%;
  height: 186rpx;
  overflow: hidden;
}
.order-icon{
  width: 480rpx;
  height: 40rpx;
  display: flex;
  align-items: center;
  margin:0 auto;
  margin-top: 30px;
  justify-content: center;
}
.order-icon .left{
  width:22rpx;
  height:22rpx;
  background:rgba(249,192,192,1);
  border:1rpx solid rgba(255, 16, 16, 1);
  border-radius:50%;
}
.order-icon .center{
  width:365rpx;
  height:2rpx;
  background:rgba(249,192,192,1);
}
.order-icon .right{
  width:40rpx;
  height:40rpx;
  background: url('data:image/png;base64,')no-repeat center center /100% 100%;
}
.order-status .icon-text{
   width: 460rpx;
  height: 40rpx;
  display: flex;
  justify-content: space-between;
  margin:0 auto;
  margin-top: 10rpx;
}
.order-status .icon-text .left,.order-status .icon-text .right{
  font-size:22rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(0,0,0,1);
}
/* order status end */
.border{
  width:100%;
  height:16rpx;
  background:rgba(246,246,246,1);
}
/* Address */
.Address{
  width: 100%;
  min-height: 208rpx;
  box-sizing: border-box;
  padding:  0 30rpx;
}
.Address .title{
  font-size:30rpx;
  font-family:PingFangSC-Medium;
  font-weight:600;
  color:rgba(0,0,0,1);
  line-height:34rpx;
  margin-top: 32rpx;
}
.Address .name{
  width: 100%;
  height: auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 36rpx;
}
.Address .name .left,.Address .name .right{
  font-size:26rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(102,102,102,1);
  line-height:34rpx;
}
.Address .content{
  margin-top: 30rpx;
  font-size:26rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(102,102,102,1);
  line-height:34rpx;
}
/* Address end */

/* Product list */
.Product-list{
  width: 100%;
  height: auto;
  border-bottom: 1rpx solid rgba(246,246,246,1);
}
.product{
  width: 100%;
  height: auto;
  box-sizing: border-box;
  padding: 0 30rpx;
}
.product .title{
  width:100%;
  font-size:28rpx;
  font-family:PingFangSC-Medium;
  font-weight:600;
  color:rgba(0,0,0,1);
  line-height:34rpx;
  margin-top: 40rpx;
}
.product .content{
  width:100%;
  height:150rpx;
  margin-top: 30rpx;
  overflow: hidden;
  display: flex;
  justify-content: space-between;
  margin-bottom: 20rpx;
}
.product .content:last-child{
  margin-bottom: 40rpx;
}
.product .content .left{
  width:140rpx;
  height:140rpx;
  border:1rpx solid rgba(238, 238, 238, 1);
  border-radius:6rpx;
  box-sizing: border-box;
  padding:1rpx;
}
.product .content .left image{
  width: 100%;
  height: 100%;
  border-radius:6rpx;
}
.product .content .right{
  width: 516rpx;
  height: 100%;
}
.product .content .right .name{
  width:517rpx;
  height:64rpx;
  font-size:26rpx;
  font-family:PingFangSC-Light;
  font-weight:300;
  color:rgba(102,102,102,1);
  line-height:34rpx;
  overflow:hidden; 
  text-overflow:ellipsis;
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2; 
}
.product .content .right .size text:first-child{
  width:134rpx;
  height:20rpx;
  font-size:20rpx;
  font-family:PingFangSC-Light;
  font-weight:300;
  color:rgba(187,187,187,1);
  line-height:34rpx;
}
.product .content .right .price {
  width:100%;
  height:19rpx;
  display: flex;
  justify-content: space-between;
}
.product .content .right .price text{
  font-size:26rpx;
  font-family:DINCond-Black;
  font-weight:400;
  color:rgba(0,0,0,1);
  line-height:34rpx;

}
/* Product list end */

/* message */
.message{
  width:100%;
  height:100rpx;
  background:rgba(255,255,255,1);
  box-sizing: border-box;
  padding: 0 28rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1rpx solid rgba(246,246,246,1);
}
.message .left{
  font-size:26rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(102,102,102,1);
}
.message .right{
 font-size:26rpx;
 font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(0,0,0,1);
}
.message2{
  width: 100%;
  height:100rpx;
  background:rgba(255,255,255,1);
  border-bottom: 1rpx solid rgba(246,246,246,1);
  box-sizing: border-box;
  padding: 0 28rpx;
  display: flex;
  align-items: center;
}
.message2 text:first-child{
  font-size:26rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(102,102,102,1);
  margin-right: 20rpx;
}
.message2 text:last-child{
  font-size:26rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:#000;
  margin-top:4rpx;
}
/* message end */